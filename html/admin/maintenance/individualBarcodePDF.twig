{% if asset %}
    {% if GET.numStickers == "8" %}
        <table>
            <tr>
                {% if GET.adamrmslogo == 1 %}
                <td style="width:0.1px;">
                    <img src="{{ CONFIG.ROOTURL }}/assets/img/logosmall.png" style="height: 60px;" />
                </td>
                {% endif %}
                <td style="width:0.1px;">
                    <img src="{{ CONFIG.ROOTURL }}/api/assets/barcodes/?barcode={{ asset.barcode.assetsBarcodes_value }}&size=60&width=1&type={{ asset.barcode.assetsBarcodes_type }}" alt="Barcode" style="margin-right: 3px;">
                </td>
                <td style="width: 0.1px; white-space:nowrap; vertical-align:middle;">
                    {% if GET.tags != 0 %}<b>{{ asset.assets_tag|aTag }}</b>{% endif %}
                    {% if GET.mass == 1 %}{{ (asset['assets_mass'] ? ' - ' ~ asset['assets_mass']|mass : (asset.assetTypes_mass != "" and asset.assetTypes_mass != 0 ? ' - ' ~ asset.assetTypes_mass|mass : '')) }}{% endif %}
                    {% if GET.tags != 0 or GET.mass == 1%}<br/>{% endif %}
                    {% if GET.typename == 1 %}{{ asset.assetTypes_name }}<br/>{% endif %}
                    {% if GET.definableField1 == 1 and asset.assetTypes_definableFields[0] != "" %}{{ asset.assetTypes_definableFields[0] }}: {{ asset['asset_definableFields_1'] }}<br/> {% endif %}
                    {% if GET.instanceName == 1 %}{{ USERDATA.instance.instances_name }}{% endif %}
                </td>
                {% if GET.instancelogo == 1 %}
                <td style="vertical-align:middle;">
                    <img src="{{CONFIG.ROOTURL}}/assets/img/logos/instances/{{ USERDATA.instance.instances_logo }}" style="height:70px">
                </td>
                {% endif %}
            </tr>
        </table>
    {% elseif GET.numStickers == "8cable" %}
        <table>
            <tr>
                {% set colcount = 1 %}
                {% if GET.adamrmslogo == 1 %}
                    {% set colcount = colcount + 1 %}
                    <td style="width:0.1px;">
                        <img src="{{ CONFIG.ROOTURL }}/assets/img/logosmall.png" style="height: 10px;" />
                    </td>
                {% endif %}
                <td style="width: 0.1px; white-space:nowrap; vertical-align:top;">
                    {% if GET.tags != 0 %}<b>{{ asset.assets_tag|aTag }}</b><br/>{% endif %}
                    {% if GET.typename == 1 %}{{ asset.assetTypes_name }}<br/>{% endif %}
                    {% if GET.definableField1 == 1 and asset.assetTypes_definableFields[0] != "" %}{{ asset.assetTypes_definableFields[0] }}: {{ asset['asset_definableFields_1'] }}<br/> {% endif %}
                    {% if GET.instanceName == 1 %}{{ USERDATA.instance.instances_name }}{% endif %}
                </td>
                {% if GET.instancelogo == 1 %}
                    {% set colcount = colcount + 1 %}
                    <td>
                        <img src="{{CONFIG.ROOTURL}}/assets/img/logos/instances/{{ USERDATA.instance.instances_logo }}" style="height:10px">
                    </td>
                {% endif %}
            </tr>
            <tr>
                <td colspan="{{ colcount }}">
                    <img src="{{ CONFIG.ROOTURL }}/api/assets/barcodes/?barcode={{ asset.barcode.assetsBarcodes_value }}&size=70&width=2&type={{ asset.barcode.assetsBarcodes_type }}" alt="Barcode" style="margin-right: 3px;width:100px;">
                </td>
            </tr>
        </table>
    {% endif %}
{% endif %}